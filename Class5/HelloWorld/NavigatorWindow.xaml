<Window x:Class="HelloWorld.NavigatorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelloWorld"
        mc:Ignorable="d"
        Title="NavigatorWindow" Height="450" Width="450">
    <Window.Background>
        <ImageBrush ImageSource="C:\Users\philg\Documents\GitHub\Greene220B\Class4\HelloWorld\images\wpf-world.png" Stretch="Fill"/>
    </Window.Background>
    
    <Grid>
        <StackPanel>
            <TextBlock Text="Enter a URL for a website to visit.  Include 'http://'"/>
            <TextBox x:Name="uxUrlTextBox"/>
            <Button x:Name="uxUrlNavigateButton" Content="Go" Click="uxUrlNavigateButton_Click"/>


        </StackPanel>
        
    </Grid>
</Window>
